    "version": "0.2.0",
    "configurations": [
        {
            "name": ".NET Core Launch (console)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceRoot}\\Tests\\BuckeyeStore.Core.Tests\\bin\\Debug\\netcoreapp1.1\\BuckeyeStore.Core.Tests.dll",
            "args": [],
            "cwd": "${workspaceRoot}\\Tests\\BuckeyeStore.Core.Tests",
            "console": "internalConsole",
            "stopAtEntry": false,
            "internalConsoleOptions": "openOnSessionStart"
        },
        {
            "name": ".NET Core Attach",
            "type": "coreclr",
            "request": "attach",
            "processId": "${command:pickProcess}"
        }
    ]
}